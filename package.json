{
    "name": "masked-input",
    "version": "1.0.0",
    "description": "Custom interface to get a masked value with presets or a mask config",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-interface"
    ],
    "directus:extension": {
        "type": "interface",
        "path": "index.js",
        "source": "src/index.ts",
        "host": "^9.17.4"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "9.18.1",
        "sass": "1.55.0",
        "typescript": "4.8.3"
    },
    "dependencies": {
        "imask": "6.4.3",
        "validator-brazil": "1.2.2",
        "vue": "3.2.39"
    }
}
